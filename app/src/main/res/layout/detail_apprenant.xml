<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="fr.fondespierre.beweb.mobile.apprenants.DetailApprenantActivity">

    <TextView
        android:id="@+id/da_textView_tel"
        android:layout_width="90dp"
        android:layout_height="25dp"
        android:layout_marginRight="8dp"
        android:text="TextView"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="54dp" />

    <ImageButton
        android:id="@+id/da_imageButton_linkedin"
        android:layout_width="45dp"
        android:layout_height="38dp"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="151dp"
        tools:layout_editor_absoluteY="124dp" />

    <ImageButton
        android:id="@+id/da_imageButton_twitter"
        android:layout_width="45dp"
        android:layout_height="38dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginRight="10dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/da_imageButton_linkedin"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/da_textView_email"
        app:layout_constraintHorizontal_bias="0.504" />

    <TextView
        android:id="@+id/da_textView_prenom"
        android:layout_width="90dp"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:id="@+id/da_imageView_photo"
        android:layout_width="90dp"
        android:layout_height="75dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/btn_star_big_on" />

    <TextView
        android:id="@+id/da_textView_url_portfolio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteY="91dp"
        tools:layout_editor_absoluteX="130dp" />

    <ListView
        android:id="@+id/da_listView_skills"
        android:layout_width="331dp"
        android:layout_height="25dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/da_textView_titre_skills" />

    <TextView
        android:id="@+id/da_textView_titre_skills"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/da_imageButton_linkedin" />

    <TextView
        android:id="@+id/da_textView_titre_projets"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/da_listView_skills"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/da_textView_titre_formations"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="151dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/da_listView_projets" />

    <TextView
        android:id="@+id/da_textView_titre_infos_persos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/da_listView_formations"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageButton
        android:id="@+id/da_imageButton_github"
        android:layout_width="45dp"
        android:layout_height="38dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginLeft="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/da_textView_url_portfolio"
        app:layout_constraintRight_toLeftOf="@+id/da_imageButton_linkedin"
        android:layout_marginRight="8dp" />

    <TextView
        android:id="@+id/da_textView_infos_persos"
        android:layout_width="331dp"
        android:layout_height="50dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="7dp"
        android:text="TextView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/da_textView_titre_infos_persos" />

    <TextView
        android:id="@+id/da_textView_nom"
        android:layout_width="90dp"
        android:layout_height="25dp"
        android:text="TextView"
        app:layout_constraintLeft_toRightOf="@+id/da_imageView_photo"
        android:layout_marginLeft="32dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/da_textView_age"
        android:layout_width="90dp"
        android:layout_height="25dp"
        android:layout_marginLeft="32dp"
        android:text="TextView"
        app:layout_constraintLeft_toRightOf="@+id/da_imageView_photo"
        tools:layout_editor_absoluteY="54dp" />

    <TextView
        android:id="@+id/da_textView_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteY="91dp"
        tools:layout_editor_absoluteX="262dp" />

    <TextView
        android:id="@+id/da_textView_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="91dp" />

    <ListView
        android:id="@+id/da_listView_projets"
        android:layout_width="331dp"
        android:layout_height="25dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/da_textView_titre_projets" />

    <ListView
        android:id="@+id/da_listView_formations"
        android:layout_width="331dp"
        android:layout_height="25dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/da_textView_titre_formations" />

</android.support.constraint.ConstraintLayout>
